import java.util.*;
class divisible_input{
    public static void main(String[]args)
    {   Scanner sc=new Scanner(System.in);
        System.out.println("enter the number:");
        int n=sc.nextInt();
        
        if (n%7==0)
        {System.out.println("divisible by 7");}
        else
        {System.out.println("not divisible by 7");}

    }
}
