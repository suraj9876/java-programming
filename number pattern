import java.util.*;
public class pattern1{
    static void pattern(int n){
        int i=0,j=0;
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=i;j++)
            {
                System.out.print(i);
            } 
            System.out.println("\r");
        }
    }
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number:");
        int n=sc.nextInt();
        
        pattern(n);
    }
}
